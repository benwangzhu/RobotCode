begin

    select BusOperation of
    case(BUSCMD_WRITE):                 -- ????作

        bus_sfloat2_(BusOut.BusIoSt + 64, BusData.Float01)
        bus_sfloat2_(BusOut.BusIoSt + 96, BusData.Float02)
