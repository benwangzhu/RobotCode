        bus_sfloat2_(BusOut.BusIoSt + 288, BusData.Float08)
        bus_sfloat2_(BusOut.BusIoSt + 320, BusData.Float09)
        bus_sfloat2_(BusOut.BusIoSt + 352, BusData.Float10)
        bus_sfloat2_(BusOut.BusIoSt + 384, BusData.Float11)
        bus_sfloat2_(BusOut.BusIoSt + 416, BusData.Float12)
        bus_sint_(BusOut.BusIoSt + 448, BusData.Int13)
        bus_sint_(BusOut.BusIoSt + 480, BusData.Int14)

        bus_sbyte_(BusOut.BusIoSt + 56, BUS_CMD254)

        if (not dout[BusOut.BusIoSt + 3]) and (not dout[BusOut.BusIoSt + 4]) then 
            dout[BusOut.BusIoSt + 3] = true
            dout[BusOut.BusIoSt + 4] = false
        else
            dout[BusOut.BusIoSt + 3] = not (dout[BusOut.BusIoSt + 3])
            dout[BusOut.BusIoSt + 4] = not (dout[BusOut.BusIoSt + 4])
        endif

        if dout[BusOut.BusIoSt + 3] then
            wait for (din[BusIn.BusIoSt + 3] = true) and (din[BusIn.BusIoSt + 4] = false)
        else
            wait for (din[BusIn.BusIoSt + 3] = false) and (din[BusIn.BusIoSt + 4] = true)
        endif
        return(OK)
