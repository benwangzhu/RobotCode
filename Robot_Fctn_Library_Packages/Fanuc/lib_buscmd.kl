        bus_sfloat2_(BusOut.BusIoSt + 160, BusData.Float04)
        bus_sfloat2_(BusOut.BusIoSt + 192, BusData.Float05)
        bus_sfloat2_(BusOut.BusIoSt + 224, BusData.Float06)
