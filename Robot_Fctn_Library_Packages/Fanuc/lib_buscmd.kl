%nolockgroup
%nobusylamp
%nopause = tpenable + command + error
%comment = 'Bus Cmd 231128'

%include kliotyps
%include include/lib_transform_t
%include include/lib_busio_t
%include include/lib_buscmd_t
%include include/lib_logs_t
%include include/lib_busio_h
%include include/lib_buscmd_h
%include include/lib_string_h
%include include/lib_motion_h
%include include/lib_logs_h

routine buscmd_wo__(BusCmdId : integer;
                    BusMstSel: integer;
                    BusIn : busin_t;
                    BusOut : busout_t) : integer     from lib_buscmd 

routine buscmd_ro__(BusCmdId : integer;
                    BusMstSel: integer;
                    BusIn : busin_t;
                    BusOut : busout_t;
                    BusTimeout : integer) : integer        from lib_buscmd 

begin

end lib_buscmd

routine buscmd_wo__
begin

    select BusMstSel of
    case(BUSCMD_MST):               -- ????????????

        BusOut.RobMsgType = BusCmdId
        bus_stell_(BusIn, BusOut)
        return(OK)
    case(BUSCMD_SLE):               -- ????????????
        BusOut.MsgType = BusCmdId
        bus_ftell_(BusIn, BusOut)
        return(OK)
    else:
        post_err(CC_UALARM, 'bus cmd param2 err', 0, CC_ABORT)
    endselect

end buscmd_wo__

routine buscmd_ro__
var
    Status            : integer
begin
    select BusMstSel of
    case(BUSCMD_MST):               -- ????????????

        -- ?? Command Id
        --
        Status = bus_rtell_(BusIn, BusOut, BusTimeout)

        bus_sbyte_(BusOut.BusIoSt + 56, 0)
        
        if (not (Status = CC_SUCCESS)) then 

            log_error_('MST', 'Read CMD[' + int_2str_(BusCmdId) + '] Err[' + int_2str_(Status) + ']')
            return(Status)
        endif
        
        if (not (BusIn.MsgType = BusCmdId)) then 

            post_err(CC_UALARM, 'MST CMD[' + int_2str_(BusCmdId) + '] New CMD[' + int_2str_(BusIn.MsgType) + '] Err', 0, CC_ABORT)
            return(NG)
        endif
    case(BUSCMD_SLE):               -- ????????????

        Status = bus_wtell_(BusIn, BusOut, BusTimeout)

        if (not (Status = CC_SUCCESS)) then 

            log_error_('SLE', 'Read CMD[' + int_2str_(BusCmdId) + '] Err[' + int_2str_(Status) + ']')
            return(Status)
        endif

        if (not (BusIn.AgentMsgType = BusCmdId)) then 

            post_err(CC_UALARM, 'SLE CMD[' + int_2str_(BusCmdId) + '] New CMD[' + int_2str_(BusIn.AgentMsgType) + '] Err', 0, CC_ABORT)
            return(NG)
        endif
    else:
        post_err(CC_UALARM, 'bus cmd param2 err', 0, CC_ABORT)
    endselect

    return(Status)
end buscmd_ro__

routine bus_cmd001_
begin

    select BusOperation of
    case(BUSCMD_WRITE):                 -- ????作

        return(buscmd_wo__(BUS_CMD001, BusMstSel, BusIn, BusOut))
    case(BUSCMD_READ):                  -- ????作
        --bus_sbyte_(BusOut.BusIoSt + 56, 0)
        return(buscmd_ro__(BUS_CMD001, BusMstSel, BusIn, BusOut, BusTimeout))
    else:
        post_err(CC_UALARM, 'bus cmd param1 err', 0, CC_ABORT)
    endselect
    
end bus_cmd001_

routine bus_cmd002_
begin

    select BusOperation of
    case(BUSCMD_WRITE):                 -- ????作

        bus_sbyte_(BusOut.BusIoSt + 64, BusData.Byte01)
        bus_sbyte_(BusOut.BusIoSt + 72, BusData.Byte02)
        bus_ssint_(BusOut.BusIoSt + 80, BusData.Short03)
        bus_ssint_(BusOut.BusIoSt + 96, BusData.Short04)
        bus_sint_(BusOut.BusIoSt + 112, BusData.Int05)
        bus_sint_(BusOut.BusIoSt + 144, BusData.Int06)
        bus_sfloat2_(BusOut.BusIoSt + 176, BusData.Float07)
        bus_sfloat2_(BusOut.BusIoSt + 208, BusData.Float08)
        return(buscmd_wo__(BUS_CMD002, BusMstSel, BusIn, BusOut))
    case(BUSCMD_READ):                  -- ????作

        if buscmd_ro__(BUS_CMD002, BusMstSel, BusIn, BusOut, BusTimeout) = CC_SUCCESS then
            BusData.Byte01 = bus_gbyte_(IO_DIN, BusIn.BusIoSt + 64)
            BusData.Byte02 = bus_gbyte_(IO_DIN, BusIn.BusIoSt + 72)
            BusData.Short03 = bus_gsint_(IO_DIN, BusIn.BusIoSt + 80)
            BusData.Short04 = bus_gsint_(IO_DIN, BusIn.BusIoSt + 96)
            BusData.Int05 = bus_gint_(IO_DIN, BusIn.BusIoSt + 112)
            BusData.Int06 = bus_gint_(IO_DIN, BusIn.BusIoSt + 144)
            BusData.Float07 = bus_gfloat2_(IO_DIN, BusIn.BusIoSt + 176)
            BusData.Float08 = bus_gfloat2_(IO_DIN, BusIn.BusIoSt + 208)
            return(OK)
        endif

        BusData.Byte01 = 0
        BusData.Byte02 = 0
        BusData.Short03 = 0
        BusData.Short04 = 0
        BusData.Int05 = 0
        BusData.Int06 = 0
        BusData.Float07 = 0.0
        BusData.Float08 = 0.0
