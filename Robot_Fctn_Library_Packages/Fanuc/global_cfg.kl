    -- log_info_('', ' 2 ==> PN IO DEVCE         [100]')
    -- log_info_('', ' 3 ==> DUAL CHNL ON IO DEV [102]')

    if chk_order_('J907') or chk_order_('S523') then

        Rack = 100
        go to LBL_IO
    endif

