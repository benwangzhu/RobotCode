    cfr_macro_(1,  'Move To Home', 'MOV_HOME', true)
    cfr_macro_(21, 'Request To Continue', 'REQ_CONT', false)
    cfr_macro_(22, 'Path Segment', 'SET_SEGM', false)
    cfr_macro_(23, 'Enter I Zone', 'ENTRIZON', false)
    cfr_macro_(24, 'Exit I Zone', 'EXITIZON', false)
    cfr_macro_(25, 'Echo Job Info', 'ECHOJOBINFO', false)
    cfr_macro_(26, 'Echo Channels', 'ECHOCHANNEL', false)
    cfr_macro_(27, 'Servo01 Ctrl', 'SERVO01CTRL', false)
