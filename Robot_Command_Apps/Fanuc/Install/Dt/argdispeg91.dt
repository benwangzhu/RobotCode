      "CMD_SEDPOS" 	    	: '4',
      "CMD_CAMERA" 	    	: '5',
      "CMD_DQMONI"          : '6'
[V01, '1']
    I02 = " PID"  : (0~500) : '0'
    I03 = " VIN R[]"  : (0~500) : '0'  
[V01, '2']
    I02 = " V PR[]"  : (0~500) : '0'
    I03 = " CODE"  : (0~255) : '0'  
[V01, '3']
    N02 = " VM SPEED"  : (1.0~1500.0) : '200.0'
    I03 = " VM DELAY(MS)"  : (0~3000) : '500' 
[V01, '4']
    I02 = " UF[]"  : (0~10) : '0'
    I03 = " ET[]"  : (0~10) : '0'
[V01, '5']
    I02 = " CAR[]"  : (0~255) : '0'
    I03 = " UF[]"  : (0~10) : '0'
    I04 = " ET[]"  : (0~10) : '0'  
[V01, '6']
