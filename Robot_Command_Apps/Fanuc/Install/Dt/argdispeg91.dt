[PROGRAM]
NAME = "SBT_COMM002"
[ARGUMENT]
V01 = "CMD_VPARM"           : '-1',
	  "CMD_TRIG" 	        : '1',
      "CMD_SCAN START"         : '2', 
      "CMD_SCAN STOP"         : '3',
      "CMD_RESULT"          : '101'
[V01, '-1']
    I02 = " PID"  : (0~500) : '0'
    I03 = " VIN R[]"  : (0~500) : '0' 
[V01, '1']
    I02 = " PICTURE ID"  : (0~255) : '0'
[V01, '2']
    I02 = " PICTURE ID"  : (0~255) : '0'
[V01, '101']
    I02 = " RESULT R[]"  : (1~500) : '211'
[ENDPROGRAM]

[PROGRAM]
NAME = "SBT_COMM003"
[ARGUMENT]
V01 = "CMD_VPARM"       : '1',
	  "CMD_SCAN" 	    : '2',
      "CMD_POS"         : '3', 
      "CMD_OVER"        : '4'
[V01, '1']
    I02 = " PID"  : (0~500) : '0'
    I03 = " VIN R[]"  : (0~500) : '0' 
[V01, '2']
    V02 = " SINGLE"        : '1', 
          " MULTI"        : '2'
        [V02, '1']
            N03 = " NUM "
        [V02, '2']
            V03 = " START"    : '1',
                  " STOP"    : '2'
            [V02, '2', V03, '1']
                N04 = " NUM " 
[V01, '3']
    N02 = " PR" 
[ENDPROGRAM]

