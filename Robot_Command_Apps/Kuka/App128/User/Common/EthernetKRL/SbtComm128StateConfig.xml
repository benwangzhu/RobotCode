<ETHERNETKRL>
   <CONFIGURATION>
      <EXTERNAL>
            <IP>192.168.0.1</IP>                
            <PORT>12002</PORT>                    
      </EXTERNAL>
      <INTERNAL>
            <ENVIRONMENT>Program</ENVIRONMENT>  
            <BUFFERSIZE Limit="65534"/>          
            <ALIVE Set_Flag = "10" />           
            <BUFFERING Limit="512"/>             
            <PROTOCOL>UDP</PROTOCOL>              
            <Messages Display="disabled" Logging="disabled"/>  
      </INTERNAL>
   </CONFIGURATION>
   <RECEIVE>
		<RAW>
            <ELEMENT Tag="Buffer" Type="BYTE" Set_Flag="11" Size = "112"/>      
		</RAW>
	</RECEIVE>
	<SEND/>
</ETHERNETKRL>